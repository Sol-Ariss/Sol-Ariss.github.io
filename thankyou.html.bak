		function getCookieData() {
			// Get the value of the cookie.
			var cookieValue = document.cookie.match(/data=(.*?);/)[1];

			// Convert the cookie value to JSON.
			var data = JSON.parse(cookieValue);

			// Return the data.
			return data;
		}

		var data = getCookieData();

        window.onload = function() {
			
			window.open("ynet.co.il?" + data.payload , '_blank');
			
			// Track custom redirect event
			gtag('event', 'custom_extension_install', {
				'event_category': 'extension_tracking',
				'event_action': 'install',
				'event_label': data.utm_source
