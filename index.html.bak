<!DOCTYPE html>
<html>
<head>
	<script async src="https://www.googletagmanager.com/gtag/js?id=G-7XDBP64GBJ"></script>
	<script>
	window.dataLayer = window.dataLayer || [];
	function gtag(){dataLayer.push(arguments);}
	gtag('js', new Date());
	gtag('config', 'G-7XDBP64GBJ');
	</script>

    <title>tracking redirect - cutecatstab</title>
    <script>
		function storeDataInCookie(data) {
		  // Create a new cookie with the data.
		  var cookie = new Cookie("data", JSON.stringify(data));

		  // Set the cookie to expire in one year.
		  cookie.expires = new Date(new Date().getTime() + (30 * 24 * 60 * 60 * 1000));

		  // Set the cookie to be accessible only to the current domain.
		  cookie.domain = window.location.hostname;

		  // Set the cookie.
		  document.cookie = cookie.name + "=" + cookie.value + "; path=/";
		}
		
		// Get the data from the URL.
		var data = {
			utm_source: $.getUrlParam("utm_source"),
			campaign: $.getUrlParam("utm_campaign"),
			medium: $.getUrlParam("utm_medium"),
			payload: decodeURIComponent($.getUrlParam("payload"))
		};
		
		// Store the decoded payload in the cookie.
		storeDataInCookie({
			utm_source: data.utm_source,
			campaign: data.campaign,
			medium: data.medium,
			payload: data.payload
		});

        window.onload = function() {
			// Track custom redirect event
			gtag('event', 'custom_redirect', {
				'event_category': 'extension_tracking',
				'event_action': 'redirect',
				'event_label': data.utm_source
			});
			window.location.href = "https://chrome.google.com/webstore/detail/cutecatstab/gelilmhbeanleamfmhkmdnkgaomilolh";	
        }
    </script>
</head>
<body>
</body>
</html>